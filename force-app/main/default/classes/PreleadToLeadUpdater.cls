public class PreleadToLeadUpdater {
    public static void updateLeadsFromPreleads(List<PreLead__c> preLeads) {
        Map<Id, Lead> leadsToUpdate = new Map<Id, Lead>();

        for (PreLead__c preLead : preLeads) {
            if (preLead.Lead__c != null) {
                Lead lead = new Lead(Id = preLead.Lead__c);
                updateLeadFromPrelead(lead, preLead);
                leadsToUpdate.put(lead.Id, lead);
            }
        }

        if (!leadsToUpdate.isEmpty()) {
            update leadsToUpdate.values();
        }
    }

